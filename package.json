{
  "name": "noisefy",
  "version": "0.1.0",
  "description": "JS library to wrap web audio funny effects",
  "author": "Arturo Galan <arturo.galan@gmail.com>",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-register --colors -w ./test/*.spec.js",
    "test:cover": "cross-env NODE_ENV=test nyc mocha --require babel-register --colors test/*.js",
    "repl": "node -i -e \"$(< ./lib/webpack-library-starter.js)\"",
    "lintfix": "eslint --fix src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arturogalan/noisefy.git"
  },
  "keywords": [
    "web",
    "audio",
    "javascript",
    "guitar",
    "pedal"
  ],
  "bugs": {
    "url": "https://github.com/arturogalan/noisefy/issues"
  },
  "homepage": "https://github.com/arturogalan/noisefy#readme",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-istanbul": "^5.2.0",
    "babel-preset-env": "^7.0.0-beta.3",
    "babel-register": "^7.0.0-beta.3",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^4.2.0",
    "jsdom": "15.2.0",
    "jsdom-global": "3.0.2",
    "mocha": "6.2.2",
    "nyc": "^14.1.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "yargs": "^14.2.0"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  }
}
